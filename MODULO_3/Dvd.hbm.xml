<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

	<class name="implementazione.Dvd"
        table="dvd">
        <id name="id" column="DVD_ID" type="long">
            <generator class="increment" />
        </id>
        
        <property name="titolo" column="TITOLO" type="string"/>
		<property name="contentDescription" column="CONTENT_DESCRIPTION" type="string" />
        <property name="formato" column="FORMATO" type="string"/>
        
	</class>

</hibernate-mapping>